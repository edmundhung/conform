{
	"compilerOptions": {
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"jsx": "react-jsx",
		"allowSyntheticDefaultImports": true,
		"noUncheckedIndexedAccess": true,
		"strict": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"composite": true,
		"skipLibCheck": true
	}
}
