{
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"target": "ES2020",
		"moduleResolution": "node16",
		"allowSyntheticDefaultImports": false,
		"noUncheckedIndexedAccess": true,
		"strict": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"composite": true,
		"skipLibCheck": true
	}
}
