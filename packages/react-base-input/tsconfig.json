{
	"compilerOptions": {
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"jsx": "react",
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"composite": true,
		"skipLibCheck": true
	}
}
